{
  "name": "backend",
  "version": "1.0.0",
  "description": "BOAT3 importer backend",
  "main": "src/app.ts",
  "scripts": {
    "test": "mocha --timeout 5000",
    "prep": "tsc --declaration && mocha --timeout 5000 && grunt bump",
    "comp": "tsc --declaration",
    "dev": "ts-node-dev -r dotenv/config --respawn --transpile-only ./src/app.ts",
    "prod": "tsc && node ./build/app.js",
    "docker": "docker build -t rongem/cmdb-backend .",
    "start": "node dist/app.js"
  },
  "keywords": [
    "BOAT"
  ],
  "author": "Michael Ronge",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-ntlm": "^2.3.3",
    "@types/mssql": "^7.1.3",
    "@types/node": "^14.17.26",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "dotenv": "^10.0.0",
    "grunt": "^1.4.1",
    "grunt-bump": "^0.8.0",
    "mocha": "^9.1.2",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-ntlm": "^2.6.1",
    "express-validator": "^6.13.0",
    "mssql": "^7.2.1"
  }
}
