    "@types/express-ntlm": "^2.3.3",
    "@types/mssql": "^9.1.0",
    "@types/node": "^20.10.1",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "dotenv": "^16.0.0",
    "grunt": "^1.4.1",
    "grunt-bump": "^0.8.0",
    "mocha": "^10.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-ntlm": "^2.6.2",
    "express-validator": "^7.0.1",
    "mssql": "^10.0.1"
