{
  "name": "backend",
  "version": "1.2.72",
  "description": "BOAT3 importer backend",
  "main": "src/app.ts",
  "scripts": {
    "test": "mocha --timeout 5000",
    "prep": "tsc --declaration && mocha --timeout 5000 && grunt bump",
